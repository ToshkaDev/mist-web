div(*ngIf="result$ | async")
  .genes-container(*ngIf="(query$ | async) && (dataSource.connect() | async).length" class="flex-container")
    mat-paginator.paginator-txt-size([length]="count" [pageSize]="perPage" [pageIndex]="currentPage-1" [pageSizeOptions]="pageSizeOptions" (page)="pageApply($event)")
    mist-genes-list(
      [genes$]="result$"
      [result]="dataSource"
      [displayedColumns]="displayedColumns"
    )